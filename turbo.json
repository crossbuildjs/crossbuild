{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["clean", "^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"lint": {},
		"clean": {
			"dependsOn": ["^clean"],
			"cache": false
		},
		"test": {
			"dependsOn": ["build"]
		},
		"dev": {
			"dependsOn": ["clean", "build"]
		}
	}
}
